{
  "version": 2,
  "builds": [
    {
      "src": "target/*.jar",
      "use": "@vercel/java",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": "target/**/*"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "target/demo-0.0.1-SNAPSHOT.jar",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}